{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
          "warn",
          {
            "extensions": [".js", ".jsx"]
          }
        ],
        "react/react-in-jsx-scope": "off",
        "comma-dangle": "off",
        "no-undef": "off",
        "react/prefer-stateless-function": "off",
        "react/state-in-constructor": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": "off"
      }
}
